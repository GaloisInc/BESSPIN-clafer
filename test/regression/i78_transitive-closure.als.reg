open util/integer
pred show {}


abstract sig c1_TimeLevel
{ r_c2_aggregatesTo : lone c2_aggregatesTo }

sig c2_aggregatesTo in c1_TimeLevel
{}
{ one @r_c2_aggregatesTo.this }

abstract sig c3_YearLevel extends c1_TimeLevel
{}
{ no this.@r_c2_aggregatesTo }

abstract sig c6_MonthLevel extends c1_TimeLevel
{}
{ (this.@r_c2_aggregatesTo) in c3_YearLevel }

abstract sig c10_WeekLevel extends c1_TimeLevel
{}
{ (this.@r_c2_aggregatesTo) in c6_MonthLevel }

one sig c14_Year2012 extends c3_YearLevel
{}

one sig c15_Jan2012 extends c6_MonthLevel
{}
{ (this.@r_c2_aggregatesTo) = c14_Year2012 }

one sig c19_Week1 extends c10_WeekLevel
{}
{ (this.@r_c2_aggregatesTo) = c15_Jan2012 }

sig c23_Week1AggregatesTo in c1_TimeLevel
{}
{ this = ((c19_Week1.@r_c2_aggregatesTo) + ((c19_Week1.@r_c2_aggregatesTo).@r_c2_aggregatesTo)) }

fact { c23_Week1AggregatesTo = (c15_Jan2012 + c14_Year2012) }
