open util/integer
pred show {}


abstract sig c1_Course
{ r_c2_taughtBy : set c2_taughtBy }

sig c2_taughtBy in c3_Teacher
{}
{ one @r_c2_taughtBy.this }

abstract sig c3_Teacher
{ r_c4_teaches : set c4_teaches }

sig c4_teaches in c1_Course
{}
{ one @r_c4_teaches.this }

fact { all  c : c1_Course | some  t : c.@r_c2_taughtBy | c in (t.@r_c4_teaches) }
fact { 5 <= #c16_course and #c16_course <= 5 }
sig c16_course extends c1_Course
{}

fact { 3 <= #c17_teacher and #c17_teacher <= 3 }
sig c17_teacher extends c3_Teacher
{}

