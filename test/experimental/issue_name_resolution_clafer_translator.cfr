
abstract Feature
        price : integer
              [price >= 0 && price <= 50]
        initializeInBootUp?
                bootupTime : integer
                [ bootupTime > 0 ]
        backgroundRunning ?
        installationROM : integer
                [installationROM >1]
        userDataROM : integer
                 [userDataROM >1]
        vmRAM : integer
                [vmRAM >1]
        energyConsumption : integer
                [energyConsumption > 0]
        xor Security
                high
                medium
                low
                // (medium)
        xor Stability
                high
                medium
                low
		// (medium)

abstract BatterySaving : Feature

 BatteryAutoCalibration : BatterySaving
        [ this.price = 0
	initializeInBootUp
        //bootuptime = XXX
        backgroundRunning
       // installationROM = XXX
       // userDataROM = XXX
       // vmRAM = XXX
        Security.high
        Stability.high
        //    Application.energyConsumption.increased
    ]