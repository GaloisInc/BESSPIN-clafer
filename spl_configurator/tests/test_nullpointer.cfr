// Showcase null pointer when using constraints

abstract IMeasurable
         footprint : integer

abstract Prevayler
	 Censor : IMeasurable ?
		[this.footprint = 50]
		[Snapshot]
	 Snapshot : IMeasurable ?
		[this.footprint = 108]
	 total_footprint : integer
	 [total_footprint =  Censor.footprint +  Snapshot.footprint]

simpleConfig : Prevayler

<< min simpleConfig.total_footprint >>