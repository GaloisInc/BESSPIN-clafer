abstract IMeasurable
	 footprint : integer
	 performance : integer


abstract LinkedList
	AbstractElement : IMeasurable
		[ this.footprint = 2]
		[ this.performance = 3]
	AbstractSort : IMeasurable ?
		[ this.footprint = 4]
		[ this.performance = 1]
	total_footprint : integer
		[ total_footprint = AbstractElement.footprint + AbstractSort.footprint]
	total_performance : integer
		[ total_performance = AbstractElement.performance + AbstractSort.performance]

simpleConfig : LinkedList

<< max simpleConfig.total_performance >>
<< min simpleConfig.total_footprint >>